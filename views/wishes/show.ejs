<% include ../partials/header %>

	<script type="text/javascript">
		$(document).ready(function() {
			// http://fancyapps.com/fancybox/#examples
			$(".fancybox").fancybox({
				maxHeight: 700,
				maxWidth: 900,
				minHeight: 350,
				minWidth: 350,
				helpers : {
	        		title: {
	            		type: 'outside',
	        		}
	    		},
	    		nextEffect: 'fade',
	    		prevEffect: 'fade'
			});
		});

		var titleSave;

		$(document).on("mouseover", ".fancybox", function() {
			titleSave = $(this).attr("title");
			$(this).attr("title", "");
		}).on("mouseout", ".fancybox", function() {
			$(this).attr("title", titleSave);
		}).on("click", ".fancybox", function() {
			$(this).attr("title", titleSave);
		});
	</script>

